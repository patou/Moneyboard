<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="moneyboard"><!-- On renome le module avec le mÃªme nom -->
	<inherits name='com.sfeir.common.gwt.sample.Moneyboard' />
	<!-- Definie une seule navigateur disponible (= moins de compilation) -->
	<!-- safari = Google Chrome ou Safari, gecko = Firefox, ie6 = Internet Explorer 6, ie8 = Internet Explorer 7 et 8, ie9 = Internet Explorer 9 -->
	<set-property name="user.agent" value="safari" />
	<set-property name="user.agent.os" value="linux"/>
	<set-property name="gxt.user.agent" value="chrome" />
	<!-- Definie une seule langue (= moins de compilation) -->
<!-- 	<set-property name="locale" value="fr" /> -->
	<!-- Active le debug avec la popup -->
	<set-property name="gwt.logging.popupHandler" value="ENABLED" />
	<set-property name="gwt.logging.developmentModeHandler" value="ENABLED" />
	
</module>
